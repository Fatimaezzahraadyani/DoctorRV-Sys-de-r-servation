<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>patientServlets</servlet-name>
  	<display-name>patientServlets</display-name>
  	<description></description>
  	<servlet-class>servlets.patientServlets</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>reservationServlet</servlet-name>
  	<display-name>reservationServlet</display-name>
  	<description></description>
  	<servlet-class>com.reservation.web.reservationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>patientServlet</servlet-name>
  	<display-name>patientServlet</display-name>
  	<description></description>
  	<servlet-class>com.patient.web.patientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>patientServlets</servlet-name>
  	<url-pattern>/patientServlets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>reservationServlet</servlet-name>
  	<url-pattern>/reservationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>patientServlet</servlet-name>
  	<url-pattern>/patientServlet</url-pattern>
  </servlet-mapping>
</web-app>
